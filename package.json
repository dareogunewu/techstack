{
  "name": "tenantguard",
  "version": "0.1.0",
  "description": "Multi-tenant Identity & Access Management Platform showcasing OAuth 2.0, OIDC, and enterprise federation",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "migrate": "tsx scripts/migrate.ts",
    "migrate:rollback": "tsx scripts/migrate-rollback.ts",
    "seed": "tsx scripts/seed.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "oauth2",
    "oidc",
    "identity",
    "authentication",
    "authorization",
    "multi-tenant",
    "saml",
    "scim",
    "iam",
    "identity-provider"
  ],
  "author": "Dare Ogunewu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dareogunewu/techstack.git"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "ioredis": "^5.3.2",
    "jose": "^5.2.0",
    "argon2": "^0.31.2",
    "zod": "^3.22.4",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "express-rate-limit": "^7.1.5",
    "rate-limit-redis": "^4.2.0",
    "cookie-parser": "^1.4.6",
    "uuid": "^9.0.1",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/cors": "^2.8.17",
    "@types/cookie-parser": "^1.4.6",
    "@types/uuid": "^9.0.7",
    "@types/node-cron": "^3.0.11",
    "@types/jest": "^29.5.11",
    "@types/supertest": "^6.0.2",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "prettier": "^3.1.1",
    "@swc/core": "^1.3.102",
    "@swc/jest": "^0.2.29"
  }
}
